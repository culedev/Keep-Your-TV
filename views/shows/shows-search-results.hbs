<h2>Resultados de la busqueda</h2>
<div class="shows">
{{#each showFound}}
<div class="each-show">
<a class="link" href="/shows/{{this.id}}/details">
        <img
        src="https://image.tmdb.org/t/p/original/{{this.poster_path}}"
        alt=""
        width="200px"
        height="300"
        />
        </a>
        <div class="show-data">
            <a class="link" href="/shows/{{this.id}}/details">
            <h3 id="title">{{this.name}}</h3>
            <p>{{this.overview}}</p>
            <p>Genre: </p>
            <p>Average rate: {{this.vote_average}}</p>
            </a>
        </div>
</div>
{{/each}}
</div>